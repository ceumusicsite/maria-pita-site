{
  "name": "maria-pita-site",
  "version": "1.0.0",
  "description": "Site oficial da artista gospel Maria Pita",
  "private": true,
  "scripts": {
    "start": "cd frontend && npm start",
    "dev": "cd frontend && npm start",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && python server.py",
    "build": "cd frontend && npm run build",
    "install": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:all": "npm run install && npm run install:backend",
    "seed": "cd backend && python seed_data.py",
    "test": "cd frontend && npm test"
  },
  "keywords": [
    "gospel",
    "music",
    "maria-pita"
  ],
